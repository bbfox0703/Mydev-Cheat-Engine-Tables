local memoryRecord = getAddressList().getMemoryRecordByID(9)

--local dateAddress = getAddress("Atelier_Escha_and_Logy.exe+1451ECC")
local dateAddress = getAddress("day_base")

function updateDate()
    local currentDateValue = readInteger(dateAddress)

    if currentDateValue then
        local year = math.floor((currentDateValue - 89) / 360) + 1
        local dayOfYear = currentDateValue % 360
        local month = math.floor(dayOfYear / 30) + 1
        local day = (dayOfYear % 30) + 1

        local deadlineDay = 120 - ((currentDateValue - 89) % 120)

        memoryRecord.Description = string.format("Year:%d Month:%02d Day:%02d (%03d)", year, month, day, deadlineDay)
    end
end

timerA15.OnTimer = updateDate
timerA15.Enabled = true
getLuaEngine().Close()

