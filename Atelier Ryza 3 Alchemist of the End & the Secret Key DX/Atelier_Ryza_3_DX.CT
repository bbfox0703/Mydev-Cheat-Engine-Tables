chk_assign_effects:
  cmp dword ptr [is_fill_empty_effects], 1
  jne after_chk_assign_effects
  cmp word ptr [rbx+1C], FFFF
  jne @F
  mov word ptr [rbx+1C], 8BB   // add fire element

@@:
  cmp word ptr [rbx+1E], FFFF
  jne @F
  mov word ptr [rbx+1E], 8BC   // add ice element

@@:
  cmp word ptr [rbx+20], FFFF
  jne @F
  mov word ptr [rbx+20], 8BD   // add thunder element

@@:
  cmp word ptr [rbx+22], FFFF
  jne @F
  mov word ptr [rbx+22], 8BE   // add wind element
after_chk_assign_effects:
  // *************************

