  cmp word ptr [rbx+16], FFFF
  jne @F
  mov word ptr [rbx+16], E

after_quality:
  //*****************************************************************************
  // 特性調整
adjust_crit_percent:
  cmp dword ptr [is_adjust_crit_p], 1
  jne @F
  push rcx
  push rax
  push rdx
  push rsi
  push rdi

  lea rdi, [offset_pairs]
  mov ecx, 3

loop_traits_start:
  // 讀取 word_offset 和 byte_offset
