  Version:
  Date   : 2023-03-27
  Author : bbfox@https://opencheattables.com
}

[ENABLE]

aobscanmodule(INJECT_BATTLE_END_HP,$process,89 74 9F 10 85 DB) // should be unique
alloc(newmem,$1000,INJECT_BATTLE_END_HP)

label(code)
label(return)
label(i_1st_HP_addr)

newmem:
  cmp rbx, 0
  jne code
  push r15
  cmp qword ptr [i_1st_HP_addr], 0
  jne next1
  lea r15, [rdi+rbx*4+10]
  mov [i_1st_HP_addr], r15

next1:
  mov r15d, [rdi+rbx*4+10+100]
  mov esi, r15d
  pop r15

code:
  mov [rdi+rbx*4+10],esi
  test ebx,ebx
  jmp return
align 10 cc
  i_1st_HP_addr:
  dq 0
INJECT_BATTLE_END_HP:
  jmp newmem
  nop
return:
registersymbol(INJECT_BATTLE_END_HP)
registersymbol(i_1st_HP_addr)

