label(code)
label(return)
label(i_min_quality_put_in_container is_assign_empty_trait1 is_modify_cc)
label(i_last_store_item_in_container_addr)
label(is_make_quality_difference is_open_all_slots is_reset_item_level)
label(is_adjust_crit_p is_assign_quality_traits)
label(is_fill_empty_effects)

newmem:
  mov [i_last_store_item_in_container_addr], rbx

  push rax
  xor rax, rax
{ $try}
  test rbx, rbx
  jz endp

  cmp word ptr [rbx+6], 0  // quality is 0
  je endp

  mov ax, [rbx+6]
  cmp ax, [i_min_quality_put_in_container]
  jae check_slots
  mov ax, [i_min_quality_put_in_container]
  cmp ax, 3E7
  jae check_slots

  // keep quality not the same (jitter)
  cmp dword ptr [is_make_quality_difference], 1
  jne next1

  inc word ptr [i_seq]
  cmp word ptr [i_seq], 2
  jbe next0
  mov word ptr [i_seq], 0
next0:

  add ax, word ptr [i_seq]
  cmp ax, 3E7
  jbe next1
  mov ax, 3E7

next1:
  mov [rbx+6], ax
  mov [rbx+8], ax

check_slots: // num of slots open
  cmp dword ptr [is_open_all_slots], 1
  jne @F

  and byte ptr [rbx+3], C7

check_level:
  cmp dword ptr [is_reset_item_level], 1
  jne @F

  cmp byte ptr [rbx+A], 5
  jbe @F
  mov byte ptr [rbx+A], 5

chk_reduce_cc:
  cmp dword ptr [is_modify_cc], 1
  jne @F

  mov byte ptr [rbx+C], -A


chk_remove_enforced: // not confirmed
  cmp dword ptr [is_remove_enforced], 1
  jne @F

