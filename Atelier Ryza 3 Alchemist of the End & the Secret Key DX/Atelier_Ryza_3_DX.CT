              <ID>31</ID>
              <Description>"Duplicate highlighted item (3x) to container (v1.00 only)"</Description>
              <Color>FF2D96</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
if memrec then print(memrec.Description) end
print("Start")

-- Check if container base address is available
local containerBaseAddr = getAddressSafe('container_base_addr')
if not containerBaseAddr then
  print("Error: container_base_addr symbol not found")
  return
end

-- Check if serial number system is available
local serialBaseAddr = getAddressSafe('item_serial_base')
if not serialBaseAddr then
  print("Error: item_serial_base symbol not found")
  return
end

local itemBaseAddr = readPointer("container_base_addr")
print(string.format("Source item addr: %08X", itemBaseAddr))

if itemBaseAddr == 0 then
  print("Please select item first")
  return
end

if not getAddressSafe('Atelier_Ryza_3_DX.exe') then
  print("Atelier_Ryza_3_DX.exe not found")
  return
end

-- Check if message symbols are available
local msgAddr1 = getAddressSafe('str_msg1')
local msgAddr2 = getAddressSafe('str_msg2')
local msgAddr3 = getAddressSafe('str_msg3')

if not msgAddr1 or not msgAddr2 or not msgAddr3 then
  print("Warning: str_msg symbols not found, messages will only be printed")
end

-- Cache frequently used values
local containerBaseAddress = g_containerBaseAddress
local MaxCount = MaxContainerCount
local ItemSize = ItemRecordSize
local dupCount = 3
local MAX_QUALITY = 999
local MAX_SERIAL = 32267  -- Hard cap at 32267
local MAX_MSG_LENGTH = 60  -- Maximum message length including null terminator

print(string.format("Container addr: %08X", containerBaseAddress))

-- Read current serial base
local currentSerialBase = readSmallInteger(serialBaseAddr)
print(string.format("Current serial base: %d", currentSerialBase))

-- Pre-read source item data
local srcItemData = readBytes(itemBaseAddr, ItemSize, true)
local srcQuality = readSmallInteger(itemBaseAddr + 0x06)

print(string.format("Source quality: %d", srcQuality))

-- Check if source quality is invalid
if srcQuality &gt; MAX_QUALITY then
  print(string.format("Error: Source quality (%d) exceeds maximum (%d)", srcQuality, MAX_QUALITY))
  print("Duplication cancelled")
  return
end

-- **Find empty slot**
local currentAddr = containerBaseAddress
local emptySlotAddr = nil

for i = 1, MaxCount - 5 do
  local currentID = readSmallInteger(currentAddr)
  if currentID == 0xFFFF and i &gt; 1 then
    emptySlotAddr = currentAddr
    print(string.format("Found empty slot #%d at %08X, src quality: %d", i, currentAddr, srcQuality))
    break
  end
  currentAddr = currentAddr + ItemSize
end

if not emptySlotAddr then
  print("No empty slots found")
  return
end

-- Helper function to write message to memory with null termination
local function writeMessage(address, message)
  if not address then return end

  -- Truncate message if too long (reserve 1 byte for null)
  if #message &gt;= MAX_MSG_LENGTH then
    message = message:sub(1, MAX_MSG_LENGTH - 1)
  end

  -- Write message string
  writeString(address, message)

  -- Ensure null termination
  writeByte(address + #message, 0)
end

-- **Duplicate items with managed serial numbers and quality**
local currentQuality = srcQuality
local msgAddresses = {msgAddr1, msgAddr2, msgAddr3}
local nextSerial = currentSerialBase

-- Check if already at or above max - cap it
if currentSerialBase &gt;= MAX_SERIAL then
  print(string.format("Serial base at/above cap (%d), locked at %d", currentSerialBase, MAX_SERIAL))
  nextSerial = MAX_SERIAL
  writeSmallInteger(serialBaseAddr, MAX_SERIAL)
end

for i = 0, dupCount - 1 do
  local destAddr = emptySlotAddr + (i * ItemSize)

  -- Write entire item data at once
  writeBytes(destAddr, srcItemData)

  -- Calculate quality for this duplicate
  local newQuality
  if currentQuality &gt;= MAX_QUALITY then
    newQuality = MAX_QUALITY  -- Lock at 999
  else
    newQuality = currentQuality + i
    if newQuality &gt; MAX_QUALITY then
      newQuality = MAX_QUALITY  -- Cap at 999
    end
  end

  -- Assign serial number and quality
  writeSmallInteger(destAddr, nextSerial)  -- Use managed serial
  writeSmallInteger(destAddr + 0x06, newQuality)  -- Quality at 0x06
  writeSmallInteger(destAddr + 0x08, newQuality)  -- Secondary quality field

  -- Create message
  local message = string.format("#%d, addr: %08X, Serial: %d, Quality: %d",
    i + 1, destAddr, nextSerial, newQuality)

  -- Print to console
  print(message)

  -- Write to corresponding symbol address
  writeMessage(msgAddresses[i + 1], message)

  -- Increment serial only if below cap
  if nextSerial &lt; MAX_SERIAL then
    nextSerial = nextSerial + 1
  end
end

-- Update serial base (will stay at MAX_SERIAL if at cap)
writeSmallInteger(serialBaseAddr, nextSerial)
print(string.format("Updated serial base: %d", nextSerial))

print("Done - Duplicated " .. dupCount .. " items")
closeLuaEngine()
