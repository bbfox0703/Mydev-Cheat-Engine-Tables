			local n = num &amp; 0xF -- ÂèñÊúÄ‰Ωé4bit
			result = hexstr:sub(n + 1, n + 1) .. result
			num = num &gt;&gt; 4 -- Âè≥Áßª4bit
		end
		return result
	end
end	
