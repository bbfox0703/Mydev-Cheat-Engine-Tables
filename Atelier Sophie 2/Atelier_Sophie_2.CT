alloc(newmem,$1000,INJECT_BATTLE_HP)

label(code)
label(return)
label(hp_damage_type)
label(enemy_damage_multi)

newmem:

code:
  cmp [rbx+0000010C+4], 0
  je its_enemy
  cmp [hp_damage_type], 0 // off
  je orig_code
  cmp [hp_damage_type], 1 // 1pt
  je one_damage
  cmp [hp_damage_type], 2 // No damage
  je zero_damage

half_damage_or_more:
  // Half
  cmp eax, 1
  je orig_code
  push rdx
  push rcx
  xor rdx, rdx
  cmp [hp_damage_type], 3
  jne half_damage_next1
  mov ecx, 2
  jmp half_damage_next2
half_damage_next1:
  mov ecx, 4
half_damage_next2:
  idiv ecx
  pop rcx
  pop rdx
  jmp orig_code

one_damage:
  mov eax, 1
  jmp orig_code
zero_damage:
  mov eax, 0
  jmp orig_code

its_enemy:
  cmp [enemy_damage_multi], 1
  je orig_code
  cmp [enemy_damage_multi], 0
  jne next1
  mov eax, 0
  jmp orig_code

next1:
  push rdx
  push rcx
  mov ecx, [enemy_damage_multi]
  mul eax
  pop rcx
  pop rdx

orig_code:
  sub [rbx+0000010C],eax


  jmp return
  align 10 cc
  hp_damage_type: // 0:off, 1:1pt, 2:No dmg, 3:Half, 4: 1/4
  dd 1
  enemy_damage_multi:
  dd 1



INJECT_BATTLE_HP:
  jmp newmem
  nop
return:

