        <CheatEntry>
          <ID>37798</ID>
          <Description>"Dup. highlighted item V4 (***Turn on Highlighted Item Editor first)"</Description>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
print("Start")
if memrec then print(memrec.Description) end

itemBaseAddr=readPointer("itemBaseAddr")
print("Base item addr: " .. string.format("%08X", itemBaseAddr))

srcQualityAddress = itemBaseAddr
--0x10F1550

--local barray ={}
--for ib = 0, 37, 1
--do
--  barray[ib] = readBytes(srcQualityAddress+ib,1)
--  print(string.format("%02X", barray[ib]))
--end

clearLuaLog()

if (getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe') == nil)
then
  print("Please select process Atelier_Lydie_and_Suelle_DX.exe first")
else
  --BaseContainerOffset=0x10C2040
  --ItemRecordSize=38
  --MaxContainerCount=4000
  local containerBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseContainerOffset

  isFound = false
  IsBypass = false
  prevID0 = 1
  currentID0 = 0
  currentQlty = 0
  dupCount = 2

  currentQualityAddress = containerBaseAddress --base item address
  MaxCount = MaxContainerCount

  local testBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseActivatorOffset
  local WeaponBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseWeaponOffset
  local ArmorBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseArmorOffset
  local GearBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseGearOffset
  local EndAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseoffsetEnd



  if (srcQualityAddress &gt;= testBaseAddress and srcQualityAddress &lt; WeaponBaseAddress)
  then
    --活性化
    currentQualityAddress = testBaseAddress --base item address for Activator / アニマ
    MaxCount = MaxActivatorCount
    dupCount = 1
  end

  if (srcQualityAddress &gt;= WeaponBaseAddress and srcQualityAddress &lt; ArmorBaseAddress)
  then
    --Weapon
    currentQualityAddress = WeaponBaseAddress
    MaxCount = MaxEquipCount
    dupCount = 1
  end

  if (srcQualityAddress &gt;= ArmorBaseAddress and srcQualityAddress &lt; GearBaseAddress)
  then
    --Armor
    currentQualityAddress = ArmorBaseAddress
    MaxCount = MaxEquipCount
    dupCount = 1
  end

  if (srcQualityAddress &gt;= GearBaseAddress and srcQualityAddress &lt; EndAddress)
  then
    --Assoc
    currentQualityAddress = GearBaseAddress
    MaxCount = MaxEquipCount
    dupCount = 1
  end

  local forbBaseAddress = getAddressSafe('Atelier_Lydie_and_Suelle_DX.exe')+BaseImportantOffset
  --do not duplicate important items
  if (srcQualityAddress &lt; testBaseAddress and srcQualityAddress &gt;= testBaseAddress)
  then
    IsBypass = true
    dupCount = 0
  end


  prevQualityAddress = currentQualityAddress

  if (IsBypass == false)
  then
    for i = 1, MaxCount-5, 1
    do
      if (isFound == false)
      then
        currentID0 = readSmallInteger(currentQualityAddress)

        if (currentID0 == 65535 and i &gt; 1)
        then
          print("Found empty slot in position #", i, ", address: #", string.format("%X", prevQualityAddress) )
          isFound = true
          for i1 = 1, dupCount, 1
          do
            print("#", i1, ", addr: ", string.format("%X", currentQualityAddress), ", New ID: ", prevID0 + 1)
            for i2 = 0, ItemRecordSize-1, 1
            do
              if (i2 == 0)
              then
                -- 0~1: item id
                prevID0 = prevID0 + 1
                writeSmallInteger(currentQualityAddress, prevID0)
              end
              if (i2 &gt; 1)
              then
                --theByte = barray[i2]
                --print(string.format("%02X", theByte))
                thebyte = readBytes(srcQualityAddress + i2, 1, false)
                writeBytes(currentQualityAddress + i2, thebyte)
              end


              if (i2 == (ItemRecordSize-1))
              then
                currentQlty = readSmallInteger(currentQualityAddress + 4)
                --adjust quality value to avoid highlight editor address bugs
                if (currentQlty &lt; 999)
                then
                  if (i1 % 2 == 0)
                  then
                    writeSmallInteger(currentQualityAddress + 4, currentQlty)
