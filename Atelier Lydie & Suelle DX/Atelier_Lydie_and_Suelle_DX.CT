      currentID = readSmallInteger(currentQualityAddress)

      if ( currentID &lt; 65535 )
      then
        if (currentID % 5 == 0)
        then
	      print("NO: " .. currentID .. ", set quality 250." )
        end
	    writeBytes(currentQualityAddress + 0x4, 0xfa)
	    writeBytes(currentQualityAddress + 0x5, 0x0)
	    --writeBytes(currentQualityAddress + 0x6, 0x7a)
  	    --writeBytes(currentQualityAddress + 0x7, 0x43)
      else
        break
      end
      currentQualityAddress = currentQualityAddress + ItemRecordSize
    end
    print("Done.")
  else
    Print("******Data checkSum error, script stopped!")
  end
end
