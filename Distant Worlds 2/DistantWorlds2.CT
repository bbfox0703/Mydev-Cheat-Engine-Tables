newmem:
  cmp dword ptr [i_ship_is_on], 1
  jne code

  //DistantWorlds.Types.Ship
  push rax
  mov ax, [EMPIRE_ID]
  cmp [rsi+00000218], ax
  pop rax
  jne check_enemy1

  push rax
  lea rax, [rsi]
  mov [i_ship_data_addr], rax
  pop rax

check1: //HullDamageLevel
  vmovss xmm15, [rsi+000001B0]
  vcomiss xmm15, [i_zero_d]
  je check2
  vmovss xmm15, [i_zero_d]
  movss [rsi+000001B0], xmm15

check2: //Fuel
  movss xmm15, xmm0
  vcomiss xmm15, [i_ship_min_fuel]
  jae check3
  vmovss xmm15, [i_ship_min_fuel]
  movss [rsi+000001A8], xmm15
  movss xmm0, xmm15

check3:  //energy
  vmovss xmm15, [rsi+000001A4]
  vcomiss xmm15, [i_ship_min_energy]
  jae check4
  vmovss xmm15, [i_ship_min_energy]
  movss [rsi+000001A4], xmm15

check4:  //speed modifier
  vmovss xmm15, [rsi+000001F8]
  vcomiss xmm15, [i_ship_speed_factor]
  jae check5
  vmovss xmm15, [i_ship_speed_factor]
  movss [rsi+000001F8], xmm15
  movss [rsi+000001FC], xmm15

check5:  // shields
  vmovss xmm15, [rsi+000001AC]
  vcomiss xmm15, [i_ship_min_shield]
  jae check6

  vmovss xmm15, [i_ship_min_shield]
  vmovss [rsi+000001AC], xmm15


check6: // command countdown
  vmovss xmm15, [rsi+00000200]
  vmovss xmm14, [i_ship_command_countdown_min]
  vmovss xmm13, [i_ship_command_countdown_min_bias]
  addss xmm14, xmm13
  vcomiss xmm15, xmm14
  jb check7
  vmovss xmm15, [i_ship_command_countdown_min]
  movss [rsi+00000200], xmm15

check7: // weapon range&amp; damage
  jmp code

check_enemy1: // shields
  cmp dword ptr [is_enemy_ship_zero_shield], 1
  jne code

  vmovss xmm15, [rsi+000001AC]
  vcomiss xmm15, [i_one_d]
  jle check_enemy2
  vmovss xmm15, [i_one_d]
  movss [rsi+000001AC], xmm15


check_enemy2:
  mov word ptr [is_enemy_ok], 1

  push rcx
  push rbx
  push rax

  //Base -&gt; Threats
  mov rcx, [rsi+100]
  cmp rcx, 0
  je enemy_endp0

  //Threats -&gt; ThreatsSystem
  mov rcx, [rcx+08]
  cmp rcx, 0
  je enemy_endp0

  mov eax, [rcx+18] //_size
  cmp rcx, 0
  je enemy_endp0

  //Threats -&gt; ThreatsSystem -&gt; _items
  mov rcx, [rcx+08]
  cmp rcx, 0
  je enemy_endp0

  //Threats -&gt; ThreatsSystem -&gt; _items -&gt; Array[0]
  mov rcx, [rcx+10]
  cmp rcx, 0
  je enemy_endp0


  //mov bx, [rcx+210]
  mov ax, [EMPIRE_ID]
  cmp [rcx+218], ax
  jne enemy_endp0

  jmp enemy_endp1
enemy_endp0: // target is not player
  mov word ptr [is_enemy_ok], 0

enemy_endp1:
  pop rax
  pop rbx
  pop rcx
  cmp word ptr [is_enemy_ok], 0
  je enemy_endp2

  {$try}
  //First target is player, modify speed modifier &amp; shield
  vmovss xmm15, [rsi+000001F8]
  vcomiss xmm15, [i_enemy_ship_speed_factor]
  jle enemy_endp1_1
  vmovss xmm15, [i_enemy_ship_speed_factor]
  movss [rsi+000001F8], xmm15
  {$except}

enemy_endp1_1:

enemy_endp2:


code:
  vmovss [rsi+000001A8],xmm0
  jmp return

align 10 cc
  i_ship_is_on:
  dd 1
  i_ship_data_addr:
  dq 0
  i_zero_d:
  dd 0
  i_one_d:
  dd (float)1
  i_500_d:
  dd (float)500
  i_ship_min_fuel:
