  is_enemy_ok:
  dw 0
  vf_hyperdrive_countdown:
  dd (float)15
  vf_hyperdrive_countdown_min:
  dd (float)5
  vf_hyperdrive_speed_threshold:
  dd (float)400
  is_hyperdrive_fast_charge:
  dd 0

INJECT_SHIP_DATA:
