              <ID>21</ID>
              <Description>"Duplicate highlighted item (3x) to container (v1.00 only)"</Description>
              <Color>FF51A8</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
if memrec then print(memrec.Description) end
print("Start")

-- Check if serial number system is available
local serialBaseAddr = getAddressSafe('item_serial_base')
if not serialBaseAddr then
  print("Error: item_serial_base symbol not found")
  return
end

local itemBaseAddr = readPointer("container_base_addr")
print(string.format("Source item addr: %08X", itemBaseAddr))

if itemBaseAddr == 0 then
  print("Please select item first")
  return
end

if not getAddressSafe('Atelier_Ryza_2_DX.exe') then
  print("Atelier_Ryza_2_DX.exe not found")
  return
end

-- Cache frequently used values
local containerBaseAddress = g_containerBaseAddress
local MaxCount = MaxContainerCount
local ItemSize = ItemRecordSize
local dupCount = 3
local MAX_SERIAL = 31765  -- Reset threshold
local MAX_QUALITY = 999

print(string.format("Container addr: %08X", containerBaseAddress))

-- Read current serial number from system
local currentSerial = readSmallInteger(serialBaseAddr)
print(string.format("Current serial base: %d", currentSerial))

-- Pre-read source item data once
local srcItemData = readBytes(itemBaseAddr, ItemSize, true)
local srcQuality = readSmallInteger(itemBaseAddr + 0x06)  -- Ryza 2: quality at 0x06

-- Find empty slot
local currentAddr = containerBaseAddress
local emptySlotAddr = nil

for i = 1, MaxCount - 5 do
  local currentID = readSmallInteger(currentAddr)
  if currentID == 0xFFFF and i &gt; 1 then
    emptySlotAddr = currentAddr
    print(string.format("Found empty slot #%d at %08X, src quality: %d", i, currentAddr, srcQuality))
    break
  end
  currentAddr = currentAddr + ItemSize
end

if not emptySlotAddr then
  print("No empty slots found")
  return
end

-- Duplicate items with managed serial numbers and quality
local nextSerial = currentSerial
local currentQuality = srcQuality

for i = 0, dupCount - 1 do
  local destAddr = emptySlotAddr + (i * ItemSize)

  -- Write entire item data at once
  writeBytes(destAddr, srcItemData)

  -- Calculate quality for this duplicate
  local newQuality
  if currentQuality &gt;= MAX_QUALITY then
    newQuality = MAX_QUALITY  -- Lock at 999
  else
    newQuality = currentQuality + i
    if newQuality &gt; MAX_QUALITY then
      newQuality = MAX_QUALITY  -- Cap at 999
    end
  end

  -- Assign managed serial number and quality
  writeSmallInteger(destAddr, nextSerial)
  writeSmallInteger(destAddr + 0x06, newQuality)  -- Quality at 0x06
  writeSmallInteger(destAddr + 0x08, newQuality)  -- Secondary quality field

  print(string.format("#%d, addr: %08X, Serial: %d, Quality: %d",
    i + 1, destAddr, nextSerial, newQuality))

  nextSerial = nextSerial + 1
end

-- Update serial base or reset if threshold exceeded
if nextSerial &gt; MAX_SERIAL then
  nextSerial = 1
  print(string.format("Serial reset: %d -&gt; 1", nextSerial - 1))
end

writeSmallInteger(serialBaseAddr, nextSerial)
print(string.format("Updated serial base: %d", nextSerial))

print("Done - Duplicated " .. dupCount .. " items")
closeLuaEngine()
{$asm}

