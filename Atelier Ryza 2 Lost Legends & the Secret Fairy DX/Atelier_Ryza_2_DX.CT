            <CheatEntry>
              <ID>26</ID>
              <Description>"Duplicate highlighted item (3x) to container (v1.00 only)"</Description>
              <Color>FF4AA5</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
if memrec then print(memrec.Description) end
print("Start")

local itemBaseAddr = readPointer("container_base_addr")
print(string.format("Source item addr: %08X", itemBaseAddr))

if itemBaseAddr == 0 then
  print("Please select item first")
  return
end

if not getAddressSafe('Atelier_Ryza_DX.exe') then
  print("Atelier_Ryza_DX.exe not found")
  return
end

-- Cache frequently used values
local containerBaseAddress = g_containerBaseAddress
local MaxCount = MaxContainerCount
local ItemSize = ItemRecordSize
local dupCount = 3
local MAX_SERIAL = 65535  -- 0xFFFF is used as empty marker, so max is 65535

print(string.format("Container addr: %08X", containerBaseAddress))

-- Pre-read source item data once
local srcItemData = readBytes(itemBaseAddr, ItemSize, true)
local srcQuality = readSmallInteger(itemBaseAddr + 0x04)

-- **First pass: Find maximum serial number across ALL items**
local maxSerialNumber = 0
local currentAddr = containerBaseAddress
for i = 1, MaxCount do
  local serialNum = readSmallInteger(currentAddr)
  if serialNum ~= 0xFFFF and serialNum &gt; maxSerialNumber then
    maxSerialNumber = serialNum
  end
  currentAddr = currentAddr + ItemSize
end

print(string.format("Max serial number found: %d", maxSerialNumber))

-- **Second pass: Find empty slot**
currentAddr = containerBaseAddress
local emptySlotAddr = nil
local emptySlotIndex = 0

for i = 1, MaxCount - dupCount do  -- Ensure enough space for all duplicates
  local currentID = readSmallInteger(currentAddr)
  if currentID == 0xFFFF then
    emptySlotAddr = currentAddr
    emptySlotIndex = i
    print(string.format("Found empty slot #%d at %08X, src quality: %d", i, currentAddr, srcQuality))
    break
  end
  currentAddr = currentAddr + ItemSize
end

if not emptySlotAddr then
  print("No empty slots found")
  return
end

-- **Duplicate items with proper serial numbers (with wrap-around)**
local qualityAdjust = srcQuality &lt; (999 - dupCount) and 1 or -1

for i = 0, dupCount - 1 do
  local destAddr = emptySlotAddr + (i * ItemSize)

  -- Calculate new serial number with wrap-around
  local newSerialNumber = maxSerialNumber + i + 1
  if newSerialNumber &gt; MAX_SERIAL then
    newSerialNumber = newSerialNumber - MAX_SERIAL - 1  -- Wrap to 0, 1, 2...
  end

  -- Write entire item data at once
  writeBytes(destAddr, srcItemData)

  -- Adjust unique fields
  writeSmallInteger(destAddr, newSerialNumber)  -- Use proper serial number
  writeSmallInteger(destAddr + 0x04, srcQuality + (i * qualityAdjust))  -- Quality

  print(string.format("#%d, addr: %08X, Serial: %d, Quality: %d",
    i + 1, destAddr, newSerialNumber, srcQuality + (i * qualityAdjust)))
end

print("Done - Duplicated " .. dupCount .. " items")
closeLuaEngine()
