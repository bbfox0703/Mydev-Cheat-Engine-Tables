                          <Description>"Duplicate highlighted item (3x) to container (v1.06 only)"</Description>
                          <Color>FF51A8</Color>
                          <VariableType>Auto Assembler Script</VariableType>
                          <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
if memrec then print(memrec.Description) end
print("Start")

local itemBaseAddr = readPointer("container_base_addr")
print(string.format("Source item addr: %08X", itemBaseAddr))

if itemBaseAddr == 0 then
  print("Please select item first")
  return
end

if not getAddressSafe('Atelier_Ryza_2_DX.exe') then
  print("Atelier_Ryza_2_DX.exe not found")
  return
end

-- Cache frequently used values
local containerBaseAddress = g_containerBaseAddress
local MaxCount = MaxContainerCount
local ItemSize = ItemRecordSize
local dupCount = 3

print(string.format("Container addr: %08X", containerBaseAddress))

-- Pre-read source item data once
local srcItemData = readBytes(itemBaseAddr, ItemSize, true)
local srcQuality = readSmallInteger(itemBaseAddr + 0x06)  -- Ryza 2: quality at 0x06

-- Find empty slot
local currentAddr = containerBaseAddress
local prevID = 1
local emptySlotAddr = nil

for i = 1, MaxCount - 5 do
  local currentID = readSmallInteger(currentAddr)
  if currentID == 0xFFFF and i &gt; 1 then
    emptySlotAddr = currentAddr
    print(string.format("Found empty slot #%d at %08X, src quality: %d", i, currentAddr, srcQuality))
    break
  end
  prevID = currentID
  currentAddr = currentAddr + ItemSize
end

if not emptySlotAddr then
  print("No empty slots found")
  return
end

-- Duplicate items using bulk memory operations
local qualityAdjust = srcQuality &lt; (999 - dupCount) and 1 or -1

for i = 0, dupCount - 1 do
  local destAddr = emptySlotAddr + (i * ItemSize)

  -- Write entire item data at once
  writeBytes(destAddr, srcItemData)

  -- Adjust unique fields
  writeSmallInteger(destAddr, prevID + i + 1)  -- Item serial number
  writeSmallInteger(destAddr + 0x06, srcQuality + (i * qualityAdjust))  -- Quality at 0x06

  print(string.format("#%d, addr: %08X, ID: %d, Quality: %d",
    i + 1, destAddr, prevID + i + 1, srcQuality + (i * qualityAdjust)))
end

print("Done - Duplicated " .. dupCount .. " items")
closeLuaEngine()
{$asm}

