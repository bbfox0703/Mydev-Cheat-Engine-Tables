local itemIDFilterHigh  = readInteger("i_item_id_filter_high")

-- 檢查參數是否讀取成功
if not itemQtyFilterLow or not itemQtyFilterHigh or not itemIDFilter or not itemIDFilterHigh then
  error("one of the symbols are invalid: i_item_cnt_low_limit, i_item_cnt_high_limit, i_item_id_filter, i_item_id_filter_high")
end

-- **處理 ID 過濾範圍邏輯**
-- 規則 5: 如果 filter_high &gt; 0 但 filter = 0，則設定 filter_high = 0
if itemIDFilterHigh &gt; 0 and itemIDFilter == 0 then
  itemIDFilterHigh = 0
  writeInteger("i_item_id_filter_high", 0)
  print("Reset i_item_id_filter_high to 0 (filter low is 0)")
