    parentRecord.Active = false
    for i = addressList.Count - 1, 0, -1 do
      local child = addressList[i]
      if child.Parent == parentRecord then
        child.delete()
      end
    end
  else
    print("⚠️ Parent record not found: " .. parentName)
  end
end)
