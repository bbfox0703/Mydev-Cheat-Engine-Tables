local function findWeaponLinkedID(serialUID)
  if not weaponMasterAddr then
    return nil, nil
  end

  local maxWeaponRecords = 80  -- 最多 80 個

  for i = 0, maxWeaponRecords - 1 do
    -- 讀取指標位址 [i_base_weapon_master_addr_1 + i*8]
    local recordPtr = readQword(weaponMasterAddr + (i * 8))
    if not recordPtr or recordPtr == 0 then
      -- 指標為 0，到達資料尾端
      return nil, nil
    end

    -- offset-8: Serial/uid (4 bytes)
    local masterUID = readInteger(recordPtr - 8)
    if masterUID == serialUID then
      -- 找到匹配，讀取 offset-4 的 Linked ID (4 bytes)
      local linkedID = readInteger(recordPtr - 4)
      return linkedID, recordPtr - 4  -- 返回值和實際地址
    end
