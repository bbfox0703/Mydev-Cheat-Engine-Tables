          <AssemblerScript>[ENABLE]
{$lua}
--NO_ACTIVATE
if syntaxcheck then return end
print("Start")
if memrec then print(memrec.Description) end

--Main script start
--BaseContainerOffset=0x1161520
--ItemRecordSize=0x30
--MaxContainerCount=2000
--  MaxBasketCount=100
--

local containerBaseAddress = 0
local currentQualityAddress = 0
local f1 = 0
local f2 = 0

getLuaEngine().MenuItem5.doClick()
--A11R_x64_Release.exe+1160268
if (getAddressSafe('A11R_x64_Release.exe') == nil)
then
  print("Please select process A11R_x64_Release.exe first")
else
  containerBaseAddress = getAddressSafe('A11R_x64_Release.exe')+BaseContainerOffset
  currentQualityAddress = containerBaseAddress
  local isOkay=true
  if (isOkay)
  then
    for i = 1, MaxContainerCount, 1
    do
      currentID = readSmallInteger(currentQualityAddress)

      if ( currentID &lt; 0xFFFF)
      then
        if (currentID % 10 == 0)
        then
	      print("NO: " .. currentID .. ", set quality..." )
        end

        f0 = readFloat(currentQualityAddress + 0x8)
        --if ( i &lt; 50 ) then
          --print(string.format("%08X", currentQualityAddress) .. " .. " .. f0)
        --end
        if ( f0 &lt; 90 ) then
          f0 = 90.5
        end
	    writeFloat(currentQualityAddress + 0x8, f0)
      else
        break
      end
      currentQualityAddress = currentQualityAddress + ItemRecordSize
    end
    print("Done.")
  end

end
