                      <Description>"Modify all item cnt to 255"</Description>
                      <VariableType>Auto Assembler Script</VariableType>
                      <AssemblerScript>[ENABLE]
{$lua}
if syntaxcheck then return end
--NO_ACTIVATE

local PARENT_NAME = "CARRYITEM"
local TARGET_VALUE = 255

local addressList = getAddressList()

synchronize(function()
  local parent = addressList.getMemoryRecordByDescription(PARENT_NAME)
  if not parent then
    print("Parent node not found: " .. PARENT_NAME)
    return
  end

  local modifiedCount = 0
  local skippedCount = 0
  local skipReasons = {
    noIsZero = 0,
    cntIsZero = 0,
    cntIs255 = 0,
    noNotFound = 0,
    cntNotFound = 0,
    readFailed = 0
  }

  -- 遍歷所有子節點
  for i = 0, parent.Count - 1 do
    local itemNode = parent.Child[i]

    if itemNode.Count &gt;= 2 then
      local noNode = nil
      local cntNode = nil

      for j = 0, itemNode.Count - 1 do
        local subNode = itemNode.Child[j]

        if subNode.Description == "no" and not noNode then
          noNode = subNode
        elseif subNode.Description == "cnt" and not cntNode then
          cntNode = subNode
        end

        if noNode and cntNode then
          break
        end
      end

      if not noNode then
        skipReasons.noNotFound = skipReasons.noNotFound + 1
        skippedCount = skippedCount + 1
      elseif not cntNode then
        skipReasons.cntNotFound = skipReasons.cntNotFound + 1
        skippedCount = skippedCount + 1
      else
        -- 直接讀取 Value 屬性
        local noValue = tonumber(noNode.Value)
        local cntValue = tonumber(cntNode.Value)

        if not noValue or not cntValue then
          skipReasons.readFailed = skipReasons.readFailed + 1
          skippedCount = skippedCount + 1
        elseif noValue == 0 then
          skipReasons.noIsZero = skipReasons.noIsZero + 1
          skippedCount = skippedCount + 1
        elseif cntValue == 0 then
          skipReasons.cntIsZero = skipReasons.cntIsZero + 1
          skippedCount = skippedCount + 1
        elseif cntValue == 255 then
          skipReasons.cntIs255 = skipReasons.cntIs255 + 1
          skippedCount = skippedCount + 1
        else
          -- 直接設定 Value
          cntNode.Value = tostring(TARGET_VALUE)
          modifiedCount = modifiedCount + 1
        end
      end
    else
      skippedCount = skippedCount + 1
    end
  end

  print(string.format("✔ Modified %d 'cnt' values to %d", modifiedCount, TARGET_VALUE))
  print(string.format("  Skipped %d items:", skippedCount))
  print(string.format("    - no is 0: %d", skipReasons.noIsZero))
  print(string.format("    - cnt is 0: %d", skipReasons.cntIsZero))
  print(string.format("    - cnt is 255: %d", skipReasons.cntIs255))
  if skipReasons.noNotFound &gt; 0 then
    print(string.format("    - no not found: %d", skipReasons.noNotFound))
  end
  if skipReasons.cntNotFound &gt; 0 then
    print(string.format("    - cnt not found: %d", skipReasons.cntNotFound))
  end
  if skipReasons.readFailed &gt; 0 then
    print(string.format("    - read failed: %d", skipReasons.readFailed))
  end
end)

closeLuaEngine()

