              <AssemblerScript Async="1">[ENABLE]
{$lua}
if syntaxcheck then return end
local baseSymbol = "_items (storage data)"
local parentName = "+Item list (do not delete this)"
local addressList = getAddressList()

-- 顏色設定（深色系，在白底下清楚可見）
local evenColor = 0x0080FF    -- 深橘色
local oddColor = 0x0060C0     -- 深橘紅色
local subColor1 = 0xFF6060    -- 淺藍色 (no)
local subColor2 = 0x60FF60    -- 淺綠色 (cnt)
local subColor3 = 0x6060FF    -- 淺紅色 (type)

-- 建立節點的通用函數
local function createNode(desc, addr, vtype, parent, color, showHex, dropdownMemrec)
  local node = addressList.createMemoryRecord()
  node.Description = desc
  if addr then
    node.Address = string.format("%X", addr)
    node.Type = vtype or vtByte
    node.ShowAsHex = showHex or false
    -- 如果有指定 dropdown，設定連結
    if dropdownMemrec then
      node.DropDownLinked = true
      node.DropDownLinkedMemrec = dropdownMemrec
    end
  else
    node.IsGroupHeader = true
    node.Options = "[moHideChildren]"
  end
  node.Parent = parent
  node.Color = color
  return node
end

-- 取得基底記錄的地址
local baseRecord = addressList.getMemoryRecordByDescription(baseSymbol)
if not baseRecord then
  print("Memory record not found: " .. baseSymbol)
  return
end

local baseAddr = getAddressSafe(baseRecord.Value)
if not baseAddr then
  print("Cannot get address from: " .. baseSymbol)
  return
end

print(string.format("Base address: %X", baseAddr))

local maxItems = 1000
local itemIndex = 0

synchronize(function()
  -- 取得或建立 parent node
  local parentRecord = addressList.getMemoryRecordByDescription(parentName)
  if not parentRecord then
    parentRecord = addressList.createMemoryRecord()
    parentRecord.Description = parentName
    parentRecord.IsGroupHeader = true
    parentRecord.Options = "[moHideChildren]"
  end

  -- 清除舊的子節點
  while parentRecord.Count &gt; 0 do
    parentRecord.Child[0].destroy()
  end

  -- 建立所有道具 nodes
  for n = 0, maxItems - 1 do
    local ptrOffset = baseAddr + 0x20 + n * 8
    local itemAddr = readQword(ptrOffset)

    -- 如果讀到 0，停止
    if not itemAddr or itemAddr == 0 then
      break
    end

    local itemColor = (itemIndex % 2 == 0) and evenColor or oddColor

    -- 建立道具群組節點 [nnn]CARRYITEM
    local itemNode = createNode(string.format("[%03d]CARRYITEM", itemIndex), nil, nil, parentRecord, itemColor)
    itemNode.Active = false  -- 預設不 active

    -- 建立子節點：no (Item ID)
    local noNode = createNode("no", itemAddr + 0x10, vtByte, itemNode, subColor1, false, "Item.ID")

    -- 建立子節點：cnt (Count)
    local cntNode = createNode("cnt", itemAddr + 0x11, vtByte, itemNode, subColor2, false, "cnt.ID")

    -- 建立子節點：type
    local typeNode = createNode("type", itemAddr + 0x14, vtDword, itemNode, subColor3, false)

    itemIndex = itemIndex + 1
  end

  parentRecord.Active = true
end)

print(string.format("✔ Created %d item records", itemIndex))
closeLuaEngine()

[DISABLE]
