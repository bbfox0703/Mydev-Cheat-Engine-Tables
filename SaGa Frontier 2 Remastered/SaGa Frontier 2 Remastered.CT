local addressList = getAddressList()

synchronize(function()
  local parent = addressList.getMemoryRecordByDescription(PARENT_NAME)
  if not parent then
    print("Parent node not found: " .. PARENT_NAME)
    return
  end

  local totalCount = parent.Count
  print(string.format("Clearing %d item nodes...", totalCount))

  -- ÊâπÊ¨°Âà™Èô§‰∏¶ÂÆöÊúüÂõûÊî∂
  local batchSize = 10
  local deleted = 0

  while parent.Count &gt; 0 do
    for i = 1, math.min(batchSize, parent.Count) do
      if parent.Count &gt; 0 then
        local child = parent.Child[parent.Count - 1]

        -- Ê∏ÖÁêÜ dropdown ÂºïÁî®
        for j = child.Count - 1, 0, -1 do
          local subChild = child.Child[j]
          if subChild.DropDownLinked then
            subChild.DropDownLinked = false
            subChild.DropDownLinkedMemrec = nil
          end
        end

        child.destroy()
        deleted = deleted + 1
      end
    end

    -- ÊØèÊâπÊ¨°ÂæåÂõûÊî∂‰∏ÄÊ¨°
    collectgarbage("collect")
  end

  parent.Active = false

  -- ÊúÄÂæåÂÜçÂõûÊî∂ÂπæÊ¨°
  collectgarbage("collect")
  collectgarbage("collect")

  print(string.format("‚úî Cleared %d item records", deleted))
end)

collectgarbage("collect")
closeLuaEngine()
